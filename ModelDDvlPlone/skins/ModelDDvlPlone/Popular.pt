      metal:use-macro="here/main_template/macros/master"
      i18n:domain="plone">
<body>

    <metal:main fill-slot="main">
    <tal:main-macro metal:define-macro="main" tal:define="text string:" >
    
       <tal:block tal:define="pPermiteEditar python: False; pPermiteEliminar python: False; pShowOrHideTrees python: False">
            <metal:block metal:use-macro="here/CabeceraEnTabla_i18n_view/macros/tCabeceraEnTabla_i18n" />
       </tal:block>
       <br/>


        <p class="documentDescription">
            Inicializacion con datos de Ejemplo
        </p>
        
        
        <tal:block tal:condition="python: request.get('ejemplo', '') == '1'">
            <tal:block tal:define="dummy python: here.populateEjemplo1()" />
        </tal:block>
        
        
        <tal:block tal:condition="python: request.get('ejemplo', '') == '2'">
            <tal:block tal:define="dummy python: here.populateEjemplo2()" />
        </tal:block>
    
       
        <tal:block tal:condition="python: request.get('ejemplo', '') == '3'">
            <tal:block tal:define="dummy python: here.populateEjemplo3()" />
        </tal:block>
  

        <tal:block tal:define="
            pURL  python: '%s/Tabular' % here.absolute_url();                  
            dummy python:request.response.redirect(pURL);" />
     
    </tal:main-macro>
</metal:main>

</body>
</html>

