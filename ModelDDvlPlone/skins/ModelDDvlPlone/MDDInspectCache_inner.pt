<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="en"
      lang="en">
<body>


    <div metal:define-macro="tInspectCacheInner" >
        
        <metal:block metal:use-macro="here/PerformanceAnalysis/macros/tSetupPerformanceAnalysis" />   

        <tal:block tal:define=" 
			pTrue                        python: True;
			pFalse                       python: False;
			pCacheName      	             request/theCacheName                   | string:;  
			global pStartTime  pStartTime |  python: here.ModelDDvlPlone_tool.fMillisecondsNow();" >
			

			
			<p>
				<a class="state-visible" id="cid_backToElement" href="#" tal:attributes="href python: '%s/' % here.absolute_url()">
					<font size="3">
						<strong tal:content="python: u'%s %s' % (
							here.ModelDDvlPlone_tool.fTranslateI18N( here, 'ModelDDvlPlone', 'ModelDDvlPlone_volver_action_label', 'Back to-',),
							here.ModelDDvlPlone_tool.fAsUnicode( here, here.Title()))" />
					</font>
				</a>
				
				&emsp;&emsp;
 				<a class="state-visible" id="cid_Refresh" href="#" tal:attributes="href python: '%s/MDDCacheStatus/#cid_MDDCache_%s_section' % ( here.absolute_url(), pCacheName,)" >
					<font size="2">
						<strong tal:content="python: here.ModelDDvlPlone_tool.fTranslateI18N( here, 'ModelDDvlPlone', 'ModelDDvlPlone_CacheStatus', 'Cache Status-',)" />
					</font>
				</a>        
				
				&emsp;&emsp;
				<a class="state-visible" id="cid_CacheDiagnostics_Link" href="#" tal:attributes="href python: '%s/MDDCacheDiagnostics/#cid_MDDCache_%s_section' % ( here.absolute_url(), pCacheName,)">
					<font size="2">
						<strong tal:content="python: here.ModelDDvlPlone_tool.fTranslateI18N( here, 'ModelDDvlPlone', 'ModelDDvlPlone_CacheDiagnostics_action_label', 'Cache Diagnostics-',)" />
					</font>
				</a>
			</p>

					
			<a class="state-visible" id="cid_Refresh" href="#" tal:attributes="href python: '%s/MDDInspectCache/?theCacheName=%s' % ( here.absolute_url(), pCacheName,)"
				tal:define="aRefreshLabel python: here.ModelDDvlPlone_tool.fTranslateI18N( here, 'ModelDDvlPlone', 'ModelDDvlPlone_refrescar_action_label', 'Refresh-',)" >
				<img src="#" alt="#" title="#" 
					tal:attributes="src python: '%s/%s' % ( here.portal_url(), 'refrescar.gif',); alt aRefreshLabel; title aRefreshLabel" 
					id="icon-refrescar" />
				<strong tal:content="python: aRefreshLabel" />
			</a>

	 
			<br/>

			<h2>
				<strong tal:content="python: here.ModelDDvlPlone_tool.fTranslateI18N( here, 'ModelDDvlPlone',  'ModelDDvlPlone_InspectCache_CacheName_%s' % pCacheName, '%s %s' % ('Name-',  pCacheName, ))" />
			</h2>
			
			<tal:block tal:replace="nothing">
				<tal:block 
					tal:define="pCacheReport python: here.ModelDDvlPlone_tool.fDumpCache_HTML( here, pCacheName, theAdditionalParams={})" 
					tal:condition="pCacheReport" 
					tal:replace="structure pCacheReport" />
			</tal:block>
			
			<tal:block 
				tal:define="pCacheReport python: here.MDDCacheDump( here.ModelDDvlPlone_tool, here, pCacheName, theAdditionalParams={})" 
				tal:condition="pCacheReport" 
				tal:replace="structure pCacheReport" />
				 
			<br/>
				
	
			<metal:block metal:use-macro="here/PerformanceAnalysis/macros/tRenderProcessingTime_HTML" />   
		
        </tal:block>
        
      
    </div>
    
    
     
    

    
	
	
	
	
	
	
	
	
	
		
	
	
	
	
    	
</body>
</html>
    