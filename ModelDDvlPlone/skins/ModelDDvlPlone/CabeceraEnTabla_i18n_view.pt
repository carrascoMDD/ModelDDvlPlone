<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="en"
      lang="en">
<body>





    <div metal:define-macro="tCabeceraEnTabla_i18n" >
        <tal:block tal:define="
            pPresentationVariability_DisplayOwnerAndContainerLabels python: True;     
            pPresentationVariability_DisplayOwnerAndContainerTypeIcons python: True;     
            pPresentationVariability_DisplayNavigationLabels python: True;  
            pPresentationVariability_KeepLabelPositionsWithWhites python: False;
        
            SRES                              SRES | nothing;
            pTrue                             python: True; 
            pFalse                            python: False; 
            pPagina                           pPagina          | string:;
            pAllowTextualAndTabular           pAllowTextualAndTabular | pFalse;
            pPaginaRefrescar                  pPaginaRefrescar | pPagina;
            pAllowRefrescar                   pAllowRefrescar | pAllowTextualAndTabular;
            pShowOrHideTrees                  pFalse;
            pShowTrees                        pShowTrees       | pFalse;
            pPermiteEditar                    pPermiteEditar   | pFalse;
            pPermiteAbandonarEditar           pPermiteAbandonarEditar   | pFalse;
            pPermiteAbandonarEliminar         pPermiteAbandonarEliminar   | pFalse;
            pPermiteAbandonarEnlazar          pPermiteAbandonarEnlazar | pFalse;
            pAbandonarAnchorName              pAbandonarAnchorName | string:;
            pPermiteAbandonarCrear            pPermiteAbandonarCrear    | pFalse;
            pPermiteEliminar                  pPermiteEliminar | pFalse;
            pPermiteEliminar                  python: pPermiteEliminar and not SRES[ 'is_root'];
            global pRelationCursorName        pRelationCursorName | string:;
            global pCurrentElementUID         pCurrentElementUID  | string:;
            pMostrarAnteriorPosterior         pMostrarAnteriorPosterior | pFalse;
            pMostrarAnteriorPosterior         python: pMostrarAnteriorPosterior and not SRES[ 'is_root'];
            pPaginaAnteriorPosterior          pPaginaAnteriorPosterior | string:;  
            pReferenceFieldName               pReferenceFieldName | string:;
            pPermiteExtraLinks                pPermiteExtraLinks | pTrue;
            global unPostfixPagina                   string:;
            global unPostfixPaginaAnteriorPosterior  string:; 
            global unPostfixPaginaRefrescar  string:;" 
            tal:condition="python: SRES">
                        

            <tal:block tal:condition="python: pPaginaRefrescar and not ( pPaginaRefrescar == 'Textual')">
                <tal:block tal:define="global unPostfixPaginaRefrescar  python: '%s/' % pPagina" />
            </tal:block>
            <tal:block tal:condition="python: pPaginaRefrescar and ( pPaginaRefrescar == 'MDDInspectCache')">
                <tal:block tal:define="global unPostfixPaginaRefrescar  python: '%s/' % 'MDDInspectCache'" />
            </tal:block>
            <tal:block tal:condition="python: pPaginaRefrescar and not ( pPaginaRefrescar in [ 'MDDInspectCache', 'Textual',])">
                <tal:block tal:define="global unPostfixPaginaRefrescar  python: '%s/' % pPaginaRefrescar" />
            </tal:block>

            <tal:block tal:condition="python: pPagina and not ( pPagina in 'Textual')">
                <tal:block tal:define="global unPostfixPagina  python: '%s/' % pPagina" />
            </tal:block>
            <!-- ACV 20091217 was  tal:condition="python: pPaginaAnteriorPosterior and not ( pPagina == 'Textual')" -->
            <tal:block tal:condition="python: pPaginaAnteriorPosterior ">
                <tal:block tal:define="global unPostfixPaginaAnteriorPosterior  python: '%s/' % pPaginaAnteriorPosterior" />
            </tal:block>
    
            <table  cellspacing="0" cellpadding="0" frame="void" >
                <tr>
                    <td align="left" valign="baseline" >
                        <h5>
                            <img src="#" tal:attributes="
                                src python: '%s/%s' % ( SRES[ 'portal_url'], SRES[ 'content_icon']);
                                title SRES/values_by_name/title/uvalue; 
                                alt SRES/values_by_name/title/uvalue"
                                alt="#" title="#" id="content_icon" />
                            <span tal:content="SRES/type_translations/translated_archetype_name" />
                            &emsp;
                        </h5>
                    </td>
 
                   <tal:block tal:condition="python: pPermiteExtraLinks and SRES[ 'extra_links']" >
                        <td align="center" valign="baseline" >
                            <tal:block tal:repeat="pExtraLink python:  SRES[ 'extra_links']" >
                                <a id="#"  href="#" title="#" class="state-visible" 
                                    tal:define="
                                        unTituloEnlace      python: pExtraLink[ 'label'] or '';
                                        unaHref             python: pExtraLink[ 'href'] or '';
                                        unIcon              python: pExtraLink.get( 'icon', '')"
                                   tal:attributes="
                                        title unTituloEnlace; 
                                        href  unaHref;
                                        id    string:cid_MDDActionLink_Extra_${repeat/pExtraLink/index};" >
                                    <img tal:condition="unIcon" src="#" alt="#" title="#"  id="#"
                                        tal:attributes="
                                            src       python: '%s/%s' % ( SRES[ 'portal_url'], unIcon,); 
                                            title     unTituloEnlace;
                                            alt       unTituloEnlace;
                                            id string:cid_MDDActionLink_Extra_${repeat/pExtraLink/index}_Icon;"
                                        />
                                    <span tal:condition="python: ( not unIcon) or pPresentationVariability_DisplayNavigationLabels"
                                        tal:content="unTituloEnlace" />
                                </a>   
                                &emsp;
                            </tal:block>
                        </td>
                    </tal:block>
                    
                    
                    <tal:block tal:condition="python: not SRES[ 'is_root']" >
                        <td align="center" valign="baseline"   tal:condition="python: SRES[ 'owner_element']" 
                            tal:define="unTituloEnlace python: '%s %s %s %s' % ( SRES[ 'owner_element'][ 'values_by_name'][ 'title'][ 'uvalue'], SRES[ 'owner_element'][ 'type_translations'][ 'translated_archetype_name'], SRES[ 'owner_element'][ 'values_by_name'][ 'description'][ 'uvalue'], SRES[ 'owner_element'][ 'type_translations'][ 'translated_type_description']);
                                        unaHref python: '%s%s#elemento-%s' % ( SRES[ 'owner_element']['url'], unPostfixPagina, SRES[ 'UID']);
                                        unIcon  python: SRES[ 'owner_element'][ 'content_icon']" >
                            <a   id="cid_MDDActionLink_Propietario"  class="state-visible" title="" 
                                href="#" title="#" tal:attributes="
                                    title unTituloEnlace; 
                                    href unaHref" >
                                <img src="#" tal:attributes="
                                    src python: '%s/%s' % ( SRES[ 'portal_url'], 'propietario.gif',); 
                                    title unTituloEnlace;
                                    alt SRES/owner_element/values_by_name/title/uvalue" 
                                    alt="" title="" id="icon-subject" />
                                <img tal:condition="pPresentationVariability_DisplayOwnerAndContainerTypeIcons" 
                                    src="#" tal:attributes="
                                    src python: '%s/%s' % ( SRES[ 'portal_url'], SRES[ 'owner_element'][ 'content_icon']);                                 
                                    title unTituloEnlace;
                                    alt SRES/owner_element/values_by_name/title/uvalue"
                                    alt="" title="" id="icon-propietario" />
                                <span tal:condition="pPresentationVariability_DisplayOwnerAndContainerLabels"
                                    tal:content="SRES/owner_element/values_by_name/title/uvalue" />
                            </a>      
                            &ensp;
                        </td>
                        <td align="center" valign="baseline" tal:condition="python: SRES[ 'container_element'] and SRES[ 'owner_element'] and SRES[ 'container_element'][ 'object'] and not ( SRES[ 'owner_element'][ 'object'] == SRES[ 'container_element'][ 'object'])" 
                            tal:define="unTituloEnlace python: '%s %s %s %s' % ( SRES[ 'container_element'][ 'values_by_name'][ 'title'][ 'uvalue'], SRES[ 'container_element'][ 'type_translations'][ 'translated_archetype_name'], SRES[ 'container_element'][ 'values_by_name'][ 'description'][ 'uvalue'], SRES[ 'container_element'][ 'type_translations'][ 'translated_type_description'])" >
                            <a  id="cid_MDDActionLink_Contenedor"   class="state-visible" title=""
                                href="#" title="#" tal:attributes="
                                    title unTituloEnlace; 
                                    href python: '%s%s#elemento-%s' % ( SRES[ 'container_element']['url'], unPostfixPagina, SRES[ 'UID'])" >
                                <img src="#" tal:attributes="
                                    src python: '%s/%s' % ( SRES[ 'portal_url'], 'contenedor.gif',); 
                                    title unTituloEnlace;
                                    alt SRES/container_element/values_by_name/title/uvalue" 
                                    alt="" title="" id="icon-subject" />
                                <img tal:condition="pPresentationVariability_DisplayOwnerAndContainerTypeIcons" 
                                    src="#" tal:attributes="
                                    src python: '%s/%s' % ( SRES[ 'portal_url'], SRES[ 'container_element'][ 'content_icon']);                                 
                                    title unTituloEnlace;
                                    alt SRES/container_element/values_by_name/title/uvalue"
                                    alt="" title="" id="icon-propietario" />
                                <span tal:condition="pPresentationVariability_DisplayOwnerAndContainerLabels"
                                    tal:content="SRES/container_element/values_by_name/title/uvalue" />
                            </a>    
                            &ensp;
                        </td>                
                    </tal:block>

                    <tal:block tal:condition="python: pPermiteEditar or pPermiteAbandonarEditar or pPermiteAbandonarEliminar or pPermiteAbandonarCrear or pPermiteAbandonarEnlazar or pPermiteEliminar or pShowOrHideTrees or pAllowTextualAndTabular or pAllowRefrescar">

                        <td align="center" >
                            <span  tal:condition="pAllowRefrescar">
                                <!-- tal:block tal:condition="python: pPaginaRefrescar in [ 'Tabular', 'Textual', 'General', 'TextualRest', 'Editar', 'Eliminar', 'Enlazar', 'MDDOrdenar', 'MDDInspectCache',]" -->

                                <tal:block tal:condition="python: pPaginaRefrescar">      
                                    <tal:block tal:condition="python: not( pPaginaRefrescar == 'Textual')">      
                                        <tal:block tal:condition="pRelationCursorName">
                                            <tal:block tal:condition="not: pCurrentElementUID">
                                                <tal:block tal:define="global unaHref python: '%s%s?theRelationCursorName=%s' % ( SRES['url'], unPostfixPaginaRefrescar, pRelationCursorName,)" />
                                            </tal:block>
                                            <tal:block tal:condition="pCurrentElementUID">
                                                <tal:block tal:define="global unaHref python: '%s%s?theRelationCursorName=%s&theCurrentElementUID=%s' % ( SRES['url'], unPostfixPaginaRefrescar, pRelationCursorName, pCurrentElementUID)" />
                                            </tal:block>
                                        </tal:block>
                                        <tal:block tal:condition="not: pRelationCursorName">
                                            <tal:block tal:condition="pReferenceFieldName">
                                                <tal:block tal:define="global unaHref python: '%s%s?theReferenceFieldName=%s' % ( SRES['url'], unPostfixPaginaRefrescar, pReferenceFieldName,)" />
                                            </tal:block>
                                            <tal:block tal:condition="not: pReferenceFieldName">
                                                <tal:block tal:define="global unaHref python: '%s%s' % ( SRES['url'], unPostfixPaginaRefrescar,)" />
                                            </tal:block>
                                        </tal:block>
                                    </tal:block>
                                    <tal:block tal:condition="python: pPaginaRefrescar == 'Textual'">      
                                        <tal:block tal:condition="pRelationCursorName">
                                            <tal:block tal:condition="not: pCurrentElementUID">
                                                <tal:block tal:define="global unaHref python: '%s?theRelationCursorName=%s' % ( SRES['url'], pRelationCursorName,)" />
                                            </tal:block>
                                            <tal:block tal:condition="pCurrentElementUID">
                                                <tal:block tal:define="global unaHref python: '%s?theRelationCursorName=%s&theCurrentElementUID=%s' % ( SRES['url'], pRelationCursorName, pCurrentElementUID,)" />
                                            </tal:block>
                                        </tal:block>
                                        <tal:block tal:condition="not: pRelationCursorName">
                                            <tal:block tal:condition="pReferenceFieldName">
                                                <tal:block tal:define="global unaHref python: '%s?theReferenceFieldName=%s' % ( SRES['url'],  pReferenceFieldName,)" />
                                            </tal:block>
                                            <tal:block tal:condition="not: pReferenceFieldName">
                                                <tal:block tal:define="global unaHref python: '%s' %  SRES['url']" />
                                            </tal:block>
                                        </tal:block>
                                    </tal:block>
                                    <tal:block 
                                        tal:define="aRefreshLabel python:  here.ModelDDvlPlone_tool.fTranslateI18N( here, 'ModelDDvlPlone', 'ModelDDvlPlone_refrescar_action_label', 'Refresh-')" >
                                        <a id="cid_MDDActionLink_Refrescar" href="#" title="#"  tal:attributes="href unaHref; title aRefreshLabel" >
                                            <img src="#" alt="#" title="#" 
                                                tal:attributes="src python: '%s/%s' % ( SRES[ 'portal_url'], 'refrescar.gif',); alt aRefreshLabel; title aRefreshLabel" 
                                                id="icon-refrescar" />
                                            <span tal:condition="pPresentationVariability_DisplayNavigationLabels"
                                                tal:content="aRefreshLabel" />        
                                        </a>
                                    </tal:block>                                    
                                    &ensp;
                                </tal:block>
                            </span>

                            <span  tal:condition="pAllowTextualAndTabular">
                                <tal:block tal:condition="python: pPagina == 'Tabular' and not ( pPaginaRefrescar in [ 'MDDOrdenar', 'MDDInspectCache',])">      
                                    <tal:block tal:define="global unaHref python: '%s' % SRES[ 'url']" >
                                        <a  id="cid_MDDActionLink_Textual" href="#" title="#"  tal:attributes="href unaHref" 
                                            i18n:domain="ModelDDvlPlone" i18n:attributes="title ModelDDvlPlone_textual_action_label">
                                            <img src="#" tal:attributes="
                                                src python: '%s/%s' % ( SRES[ 'portal_url'], 'textual.gif',)"  
                                                alt="Vista Textual" title="Vista Textual" id="icon-textual"
                                                i18n:domain="ModelDDvlPlone" i18n:attributes="alt ModelDDvlPlone_textual_action_label; title ModelDDvlPlone_textual_action_label" />
                                            <span tal:condition="pPresentationVariability_DisplayNavigationLabels"
                                                i18n:domain="ModelDDvlPlone" i18n:translate="ModelDDvlPlone_textual_action_label">Vista Textual</span>        
                                        </a>
                                    </tal:block>                                    
                                    &ensp;
                                </tal:block>
                                <tal:block tal:condition="python: pPagina in [ 'Textual', 'TextualRest', 'MDDOrdenar', 'MDDInspectCache', 'MDDChanges',] or ( pPaginaRefrescar in [ 'MDDOrdenar', 'MDDInspectCache','MDDChanges',])">      
                                    <tal:block tal:define="global unaHref python: '%sTabular/' % SRES[ 'url']" >
                                        <a id="cid_MDDActionLink_Tabular"  href="#" title="#"  tal:attributes="href unaHref" 
                                             i18n:domain="ModelDDvlPlone" i18n:attributes="title ModelDDvlPlone_tabular_action_label">
                                            <img src="#" tal:attributes="
                                                src python: '%s/%s' % ( SRES[ 'portal_url'], 'tabular.gif',)" 
                                                alt="Vista Tabular" title="Vista Tabular" id="icon-tabular"
                                                 i18n:domain="ModelDDvlPlone" i18n:attributes="alt ModelDDvlPlone_tabular_action_label; title ModelDDvlPlone_tabular_action_label" />
                                            <span tal:condition="pPresentationVariability_DisplayNavigationLabels"
                                                i18n:domain="ModelDDvlPlone" i18n:translate="ModelDDvlPlone_tabular_action_label">Vista Tabular</span>        
                                        </a>
                                    </tal:block>
                                    &ensp;
                                </tal:block>
                            </span>
                            <span tal:condition="pShowOrHideTrees">
                                <tal:block tal:condition="pShowTrees">      
                                    <tal:block tal:define="global unaHref python: '%s%s?HideTrees' % ( SRES[ 'url'], unPostfixPagina)" >
                                        <a id="cid_MDDActionLink_MostrarListas" href="#" title="#"  tal:attributes="href unaHref" 
                                            i18n:domain="ModelDDvlPlone"  >
                                            <img src="#" tal:attributes="
                                                src python: '%s/%s' % ( SRES[ 'portal_url'], 'mostrarlistas.gif',)"  
                                                alt="Mostrar Listas" title="HideTrees" id="icon-hidetrees"
                                                i18n:domain="ModelDDvlPlone" i18n:attributes="alt ModelDDvlPlone_hidetrees_action_label; title ModelDDvlPlone_hidetrees_action_label" />
                                            <span tal:condition="pPresentationVariability_DisplayNavigationLabels"
                                                i18n:domain="ModelDDvlPlone" i18n:translate="ModelDDvlPlone_hidetrees_action_label">Mostrar Listas</span>        
                                        </a>
                                    </tal:block>                                    
                                    &ensp;
                                </tal:block>
                                <tal:block tal:condition="python: not pShowTrees">                            
                                    <tal:block tal:define="global unaHref python: '%s%s?ShowTrees' % ( SRES[ 'url'], unPostfixPagina)" >
                                        <a id="cid_MDDActionLink_MostrarArboles" href="#" title="#"  tal:attributes="href unaHref" 
                                             i18n:domain="ModelDDvlPlone" i18n:attributes="title ModelDDvlPlone_showtrees_action_label">
                                            <img src="#" tal:attributes="
                                                src python: '%s/%s' % ( SRES[ 'portal_url'], 'mostrararboles.gif',)"  
                                                alt="Mostrar Arboles" title="HideTrees" id="icon-hidetrees"
                                                 i18n:domain="ModelDDvlPlone" i18n:attributes="alt ModelDDvlPlone_showtrees_action_label; title ModelDDvlPlone_showtrees_action_label" />
                                            <span tal:condition="pPresentationVariability_DisplayNavigationLabels"
                                                i18n:domain="ModelDDvlPlone" i18n:translate="ModelDDvlPlone_showtrees_action_label">Mostrar Arboles</span>        
                                        </a>
                                    </tal:block>
                                    &ensp;
                                </tal:block>
                            </span>
                        </td>
                        <td align="center">
                            <span  tal:condition="python: pPermiteEditar and not pPermiteAbandonarEditar and not pPermiteAbandonarEliminar and not pPermiteAbandonarCrear and not pPermiteAbandonarEnlazar">
                                <a id="cid_MDDActionLink_Editar"  href="#" title="#" tal:attributes="href python: '%sEditar/' % SRES[ 'url']" 
                                     i18n:domain="ModelDDvlPlone" i18n:attributes="title ModelDDvlPlone_editar_action_label">
                                    <img src="#" tal:attributes="
                                                src python: '%s/%s' % ( SRES[ 'portal_url'], 'edit.gif',)"  
                                        alt="Editar" title="Editar" id="icon-edit"
                                        i18n:domain="ModelDDvlPlone" i18n:attributes="alt ModelDDvlPlone_editar_action_label; title ModelDDvlPlone_editar_action_label" />
                                    <span tal:condition="pPresentationVariability_DisplayNavigationLabels"
                                        i18n:domain="ModelDDvlPlone" i18n:translate="ModelDDvlPlone_editar_action_label">Editar</span>        
                                </a>
                                &ensp;
                            </span>
                            <span  tal:condition="pPermiteAbandonarEditar">
                                <a id="cid_MDDActionLink_AbandonarEditar" href="#" title="#" tal:attributes="href python: '%sTabular/#valores' % SRES[ 'url']" 
                                     i18n:domain="ModelDDvlPlone" i18n:attributes="title ModelDDvlPlone_abandonaredicion_action_label">
                                    <img src="" alt="Abandonar Editar" title="Abandonar Edicion" id="icon-abandon-edit"
                                        tal:attributes="
                                                src python: '%s/%s' % ( SRES[ 'portal_url'], 'mostrarlistas.gif',)"                                      
                                        i18n:domain="ModelDDvlPlone" i18n:attributes="alt ModelDDvlPlone_abandonaredicion_action_label; title ModelDDvlPlone_abandonaredicion_action_label" />
                                    <span i18n:domain="ModelDDvlPlone" i18n:translate="ModelDDvlPlone_abandonaredicion_action_label">Abandonar Edicion</span>        
                                </a>
                                &ensp;
                            </span>
                            <span tal:condition="pPermiteAbandonarEnlazar">
                                <a id="cid_MDDActionLink_AbandonarEnlazar" href="#" title="#" tal:attributes="href python: '%sTabular/%s' % ( SRES[ 'url'], pAbandonarAnchorName)" 
                                     i18n:domain="ModelDDvlPlone" i18n:attributes="title ModelDDvlPlone_abandonarenlazar_action_label">
                                    <img src="" alt="Abandonar Enlazar" title="Abandonar Enlazar" id="icon-abandon-edit"
                                        tal:attributes="
                                                src python: '%s/%s' % ( SRES[ 'portal_url'], 'mostrarlistas.gif',)"                                      
                                        i18n:domain="ModelDDvlPlone" i18n:attributes="alt ModelDDvlPlone_abandonarenlazar_action_label; title ModelDDvlPlone_abandonarenlazar_action_label" />
                                    <span i18n:domain="ModelDDvlPlone" i18n:translate="ModelDDvlPlone_abandonarenlazar_action_label">Abandonar Enlazar</span>        
                                </a>
                                &emsp;
                            </span>
                            <span tal:condition="pPermiteAbandonarEliminar">
                                <a id="cid_MDDActionLink_AbandonarEliminar" href="#" title="#" tal:attributes="href python: '%sTabular/%s' % ( SRES[ 'url'], pAbandonarAnchorName)" 
                                     i18n:domain="ModelDDvlPlone" i18n:attributes="title ModelDDvlPlone_abandonareliminar_action_label">
                                    <img src="" alt="Abandonar Eliminar" title="Abandonar Eliminar" id="icon-abandon-edit"
                                        tal:attributes="
                                                src python: '%s/%s' % ( SRES[ 'portal_url'], 'mostrarlistas.gif',)"                                      
                                        i18n:domain="ModelDDvlPlone" i18n:attributes="alt ModelDDvlPlone_abandonareliminar_action_label; title ModelDDvlPlone_abandonareliminar_action_label" />
                                    <span i18n:domain="ModelDDvlPlone" i18n:translate="ModelDDvlPlone_abandonareliminar_action_label">Abandonar eliminar</span>        
                                </a>
                                &ensp;
                            </span>                        
                            <span  tal:condition="pPermiteAbandonarCrear">
                                <a  id="cid_MDDActionLink_AbandonarCrear" href="#" title="#" tal:attributes="href python: '%sTabular/%s' % ( SRES[ 'url'], pAbandonarAnchorName)"
                                     i18n:domain="ModelDDvlPlone" i18n:attributes="title ModelDDvlPlone_abandonarcreacion_action_label">
                                    <img src="" alt="Abandonar Crear" title="Abandonar Crear" id="icon-abandon-create"
                                        tal:attributes="
                                                src python: '%s/%s' % ( SRES[ 'portal_url'], 'mostrarlistas.gif',)"                                      
                                        i18n:domain="ModelDDvlPlone" i18n:attributes="alt ModelDDvlPlone_abandonarcreacion_action_label; title ModelDDvlPlone_abandonarcreacion_action_label" />
                                    <span i18n:domain="ModelDDvlPlone" i18n:translate="ModelDDvlPlone_abandonarcreacion_action_label">Abandonar Creacion</span>        
                                </a>
                                &ensp;
                            </span>
                            <span tal:condition="python: pPresentationVariability_KeepLabelPositionsWithWhites and not (pPermiteEditar or pPermiteAbandonarEditar or pPermiteAbandonarCrear)">
                                <font color="White">
                                    <img src="#" tal:attributes="
                                                src python: '%s/%s' % ( SRES[ 'portal_url'], 'blank_icon.gif',)"  
                                        alt="Editar" title="Editar" id="icon-blank"
                                        i18n:domain="ModelDDvlPlone" i18n:attributes="alt ModelDDvlPlone_editar_action_label; title ModelDDvlPlone_editar_action_label" />
                                    <span tal:condition="pPresentationVariability_DisplayNavigationLabels"
                                        i18n:domain="ModelDDvlPlone" i18n:translate="ModelDDvlPlone_editar_action_label">Editar</span>        
                                </font>
                                &emsp;
                            </span>
                            <span tal:condition="pPermiteEliminar">
                                &emsp;
                                <a  id="cid_MDDActionLink_Eliminar" href="#" title="#" tal:attributes="href python: '%sEliminar/' % SRES[ 'url']" 
                                     i18n:domain="ModelDDvlPlone" i18n:attributes="title ModelDDvlPlone_eliminar_action_label" >
                                    <img src="#" tal:attributes="
