    <div metal:define-macro="tCacheStatusInner" >

    <metal:block metal:use-macro="here/PerformanceAnalysis/macros/tSetupPerformanceAnalysis" />   

    <tal:block tal:define=" 
        pTrue                        python: True;
        pFalse                       python: False;
        pPagina                      string:Tabular;
        pPaginaAnteriorPosterior     string:Tabular;
        pShowTrees                   pFalse;
        pCacheName      	         request/form/theCacheName                   | string:;  
        pMaxCharsCached	             request/form/theMaxCharsCached              | string:;  
        pMinThresholdCharsToRelease  request/form/theMinThresholdCharsToRelease  | string:;  
        pExpireAfterSeconds          request/form/theExpireAfterSeconds          | string:;  
        pForceExpire                 request/form/theForceExpire                 | string:;  
        pDisplayCacheHitInformation  request/form/theDisplayCacheHitInformation  | string:;  
        pPeersToNotify               request/form/thePeersToNotify               | string:;  
        pIdentificationStringForPeers  request/form/theIdentificationStringForPeers   | string:;  
        pAuthenticationStringForPeers  request/form/theAuthenticationStringForPeers   | string:;  
        pAuthenticationStringFromPeers request/form/theAuthenticationStringFromPeers  | string:;  
        pChangeCacheParameters       request/form/theChangeCacheParameters       | string:; 
        pCacheDiskEnabled            request/form/theCacheDiskEnabled            | string:; 
        pCacheDiskPath               request/form/theCacheDiskPath               | string:; 
        pExpireDiskAfterSeconds      request/form/theExpireDiskAfterSeconds      | string:;  
        pEnableCache                 request/form/theEnableCache                 | string:; 
        pDisableCache                request/form/theDisableCache                | string:; 
        pResetCache                  request/form/theResetCache                 | string:; 
        pFlushCache                  request/form/theFlushCache                  | string:; 
        pFlushDiskCache              request/form/theFlushDiskCache              | string:; 
        pActivateCaching             request/form/theActivateCaching             | string:;  
        pDeactivateCaching           request/form/theDeactivateCaching           | string:;  
        pRepresentation              request/form/theRepresentation              | string:;  

    global pStartTime  pStartTime |  python: here.MDDModelDDvlPlone_tool.fMillisecondsNow();" >

    <p>
        <a class="state-visible" id="cid_backToElement" href="#" tal:attributes="href python: '%s/' % here.absolute_url()">
            <font size="3">
                <strong tal:content="python: u'%s %s' % (
                    here.MDDModelDDvlPlone_tool.fTranslateI18N( here, 'ModelDDvlPlone', 'ModelDDvlPlone_volver_action_label', 'Back to-',),
                    here.MDDModelDDvlPlone_tool.fAsUnicode( here, here.Title()))" />
            </font>
        </a>
    </p>

    <tal:block tal:condition="python: pChangeCacheParameters or pActivateCaching or pDeactivateCaching" >
        <tal:block tal:define="
            global pRetrievalStartTime  python: here.MDDModelDDvlPlone_tool.fMillisecondsNow();
            unosArgs python: {
                'theContextualObject'               :   here, 
                'theEditedCacheParameters'  : { 
                    'theActivateCaching':               pActivateCaching,
                    'theDeactivateCaching':             pDeactivateCaching,
                    'theEnableCache':                   pEnableCache,
                    'theDisableCache':                  pDisableCache,
                    'theResetCache':                    pResetCache,
                    'theFlushCache':                    pFlushCache,		
                    'theFlushDiskCache':                pFlushDiskCache,
                    'theMaxCharsCached':                pMaxCharsCached, 
                    'theMinThresholdCharsToRelease':    pMinThresholdCharsToRelease,
                    'theExpireAfterSeconds':	        pExpireAfterSeconds,
                    'theForceExpire':	                pForceExpire,
                    'theDisplayCacheHitInformation':    pDisplayCacheHitInformation, 
                    'theExpireDiskAfterSeconds':	    pExpireDiskAfterSeconds,
                    'theCacheDiskEnabled':	            pCacheDiskEnabled,
                    'theCacheDiskPath':                 pCacheDiskPath,
                    'thePeersToNotify':                 pPeersToNotify,
                    'theIdentificationStringForPeers':  pIdentificationStringForPeers,
                    'theAuthenticationStringForPeers':  pAuthenticationStringForPeers,
                    'theAuthenticationStringFromPeers': pAuthenticationStringFromPeers,
                },
                'theCacheName'                       : pCacheName,
            }; 
            pConfigureResult python: here.MDDModelDDvlPlone_tool.fConfigureTemplatesCache( **unosArgs);
            pRetrievalEndTime  python: here.MDDModelDDvlPlone_tool.fMillisecondsNow()" >	

    <tal:block tal:condition="python: pConfigureResult and pConfigureResult[ 0]">
        <div class="portalMessage" tal:content="python: '%s (%s %s)' % ( 
            here.MDDModelDDvlPlone_tool.fTranslateI18N( here, 'ModelDDvlPlone', 'ModelDDvlPlone_CacheHasBeenConfigured', 'Cache Has Been Configured-',),
            test( ( len( pConfigureResult) > 1) and pConfigureResult[ 1], here.MDDModelDDvlPlone_tool.fTranslateI18N( here, 'plone', pConfigureResult[ 1], pConfigureResult[ 1],), ''),
            test( ( len( pConfigureResult) > 2) and pConfigureResult[ 1], here.MDDModelDDvlPlone_tool.fTranslateI18N( here, 'ModelDDvlPlone', 'ModelDDvlPlone_CacheStatus_CacheName_%s' % pConfigureResult[ 2], pConfigureResult[ 2],), ''),)" />
    </tal:block>
    <tal:block tal:condition="python: not ( pConfigureResult and pConfigureResult[ 0])">
        <div class="portalStopMessage" tal:content="python: '%s (%s %s)' % (
            here.MDDModelDDvlPlone_tool.fTranslateI18N( here, 'ModelDDvlPlone', 'ModelDDvlPlone_CacheHasNotBeenConfigured', 'Cache Has NOT Been Configured-',),
            test( ( len( pConfigureResult) > 1) and pConfigureResult[ 1], here.MDDModelDDvlPlone_tool.fTranslateI18N( here, 'plone', pConfigureResult[ 1], pConfigureResult[ 1],), ''),
            test( ( len( pConfigureResult) > 2) and pConfigureResult[ 1], here.MDDModelDDvlPlone_tool.fTranslateI18N( here, 'ModelDDvlPlone', 'ModelDDvlPlone_CacheStatus_CacheName_%s' % pConfigureResult[ 2], pConfigureResult[ 2],), ''),)" />
    </tal:block>
</tal:block>
</tal:block>


    <tal:block tal:replace="nothing">
        ACV 20091127 Removed: Actions To Flush, Enable and Disable Cache are performed within the fConfigureTemplatesCache method
    </tal:block>



    <tal:block tal:replace="nothing">  
        <metal:block metal:use-macro="here/PerformanceAnalysis/macros/tRenderProfilingResults_HTML" />   
    </tal:block>								


    <tal:block tal:define="

    pRetrievalStartTime  python: here.MDDModelDDvlPlone_tool.fMillisecondsNow();
    pCacheStatusReport python: here.MDDModelDDvlPlone_tool.fRetrieveCacheStatusReport( here, theRepresentation=pRepresentation);
    pRetrievalEndTime  python: here.MDDModelDDvlPlone_tool.fMillisecondsNow();
    pPagina string:;
    pPaginaRefrescar string:MDDCacheStatus" 
    tal:condition="python: pCacheStatusReport" >


    <p tal:condition="python: pCacheStatusReport.get( 'CanDiagnose', False)" >
        <a class="state-visible" id="cid_CacheDiagnostics_Link" href="#" tal:attributes="href python: '%s/MDDCacheDiagnostics/' % ( here.absolute_url(),)">
            <font size="2">
                <strong tal:content="python: here.MDDModelDDvlPlone_tool.fTranslateI18N( here, 'ModelDDvlPlone', 'ModelDDvlPlone_CacheDiagnostics_action_label', 'Cache Diagnostics-',)" />
            </font>
        </a>
    </p>


    <form tal:define="pName python: 'cid_MDDRefreshCacheStatus_form';
        aDeltaLabel python: here.MDDModelDDvlPlone_tool.fTranslateI18N( here, 'ModelDDvlPlone', 'ModelDDvlPlone_Cache_Delta', 'Delta-',)"
        tal:attributes="name pName; id pName"
        method="post" enctype="multipart/form-data" id="#" name="#" >


    <input name="theRepresentation" type="hidden" 
        tal:define="pRepresentation pCacheStatusReport/reports_repr"
        tal:attributes="value pRepresentation" >
    <tal:block  tal:condition="python: pCacheStatusReport.get( 'IsCachingActive', False)" >
        <span>
            <font size="3" style="color: red">
                <strong tal:content="python: here.MDDModelDDvlPlone_tool.fTranslateI18N( here, 'ModelDDvlPlone', 'ModelDDvlPlone_Cache_IsActive', 'Cache Is Active-',)" />
            </font>
        </span>
    </tal:block>
    <tal:block  tal:condition="python: not pCacheStatusReport.get( 'IsCachingActive', False)" >
        <span>
            <font size="3" style="color: red">
                <strong tal:content="python: here.MDDModelDDvlPlone_tool.fTranslateI18N( here, 'ModelDDvlPlone', 'ModelDDvlPlone_Cache_IsInactive', 'Cache Is Inactive-',)" />
            </font>
        </span>
    </tal:block>
    &emsp;&emsp;&emsp;

    <a class="state-visible" id="cid_Refresh" href="#" tal:attributes="href python: '%s/MDDCacheStatus/' % here.absolute_url()"
        tal:define="aRefreshLabel python: here.MDDModelDDvlPlone_tool.fTranslateI18N( here, 'ModelDDvlPlone', 'ModelDDvlPlone_refrescar_action_label', 'Refresh-',)" >
        <img src="#" alt="#" title="#" 
            tal:attributes="src python: '%s/%s' % ( here.portal_url(), 'refrescar.gif',); alt aRefreshLabel; title aRefreshLabel" 
            id="icon-refrescar" />
        <strong tal:content="python: aRefreshLabel" />
    </a>
    &emsp;&emsp;&emsp;

    <input type="submit" name="form_submit" value="Save" id="cid_MDDRefreshCacheStatus_Refresh"					
        style="color: green; font-size: 12pt; font-style: italic; font-weight: 700"
        tal:attributes="value aDeltaLabel" />


    </form>




    <metal:block metal:use-macro="here/MDDCacheStatus_inner/macros/tAllCachesReportAndEditor" />   

    <br/>

    <tal:block 
        tal:define="pCacheReports python: pCacheStatusReport.get( 'reports', [])"
        tal:repeat="pCacheReport python: pCacheReports" >

    <metal:block metal:use-macro="here/MDDCacheStatus_inner/macros/tCacheReportAndEditor" />   

    <br/>

    </tal:block>
    <br/>

    </tal:block>


    <metal:block metal:use-macro="here/PerformanceAnalysis/macros/tRenderProcessingTime_HTML" />   

    </tal:block>


    </div>




















    <div metal:define-macro="tCacheReportAndEditor" >
        <tal:block 
            tal:define="pCacheReport pCacheReport | nothing"
            tal:condition="pCacheReport" >

    <tal:block tal:define="
        pMetaInfo  python: pCacheReport.get( 'meta_info', {});
        pCacheName python: pCacheReport.get( 'CacheName', ''); 
        pCacheKind python: pCacheReport.get( 'CacheKind', '')" >
        <br/>
        <h2 id="#" tal:attributes="id python: 'cid_MDDCache_%s_section' % pCacheName">
            <strong><span tal:content="python: here.MDDModelDDvlPlone_tool.fTranslateI18N( here, 'ModelDDvlPlone',  'ModelDDvlPlone_CacheStatus_CacheName_%s' % pCacheKind, '%s %s' % ('Kind-',  pCacheKind, ))" /></strong>
            &emsp;
            <strong tal:condition="python: not( pCacheName == pCacheKind)">
                <span tal:content="python: here.MDDModelDDvlPlone_tool.fTranslateI18N( here, 'ModelDDvlPlone',  'ModelDDvlPlone_CacheStatus_CacheName_%s' % pCacheName, '%s %s' % ('Name-',  pCacheName, ))" />
            </strong>
        </h2>
        <form tal:define="pName python: 'cid_MDDCache_%s_Parameters_form' % pCacheName"
            tal:attributes="name pName; id pName"
            method="post" enctype="multipart/form-data" id="#" name="#" >

    <input originalvalue="#" name="theChangeCacheParameters" value="#" type="hidden"
        tal:attributes="
            originalvalue python: test( pCacheStatusReport.get( 'CanConfigure', False), 'on', ''); 
            value python: test( pCacheStatusReport.get( 'CanConfigure', False), 'on', '');">

    <input originalvalue="#" name="theCacheName" value="#" type="hidden" 
        tal:attributes="originalvalue pCacheName; value pCacheName" >


    <table tal:define=" global unIndexClassFila python: 0; unasClasesFilas  python: ('odd','even')"
        class="listing" id="#" summary="#" 
        tal:attributes="
            id python: 'cid_MDDCacheParameters_%s_table' % pCacheName;
            summary python: '%s Cache Parameters' % pCacheName">
        <thead>
            <tr>
                <th align="left"  class="nosort" >
                    <font size="3" color="red">
                        <strong>
                            <span tal:condition="python: pCacheReport.get( 'CacheEnabled', False)"
                                i18n:domain="ModelDDvlPlone" i18n:translate="ModelDDvlPlone_InspectCache_Enabled">Cache Enabled-</span>
                            <span tal:condition="python: not pCacheReport.get( 'CacheEnabled', False)"
                                i18n:domain="ModelDDvlPlone" i18n:translate="ModelDDvlPlone_InspectCache_Disabled">Cache Disabled-</span>
                        </strong>
                    </font>
                    &ensp;
                    <a class="state-visible" id="cid_Refresh" href="#" tal:attributes="href python: '%s/MDDCacheStatus/&theMS=%d#cid_MDDCache_%s_section' % ( here.absolute_url(), here.MDDModelDDvlPlone_tool.fMillisecondsNow(), pCacheName, )"
                        tal:define="aRefreshLabel python: here.MDDModelDDvlPlone_tool.fTranslateI18N( here, 'ModelDDvlPlone', 'ModelDDvlPlone_refrescar_action_label', 'Refresh-',)" >
                        <img src="#" alt="#" title="#" 
                            tal:attributes="src python: '%s/%s' % ( here.portal_url(), 'refrescar.gif',); alt aRefreshLabel; title aRefreshLabel" 
                            id="icon-refrescar" />
                        <strong tal:content="python: aRefreshLabel" />
                    </a>

    </th>
    <th align="right"  class="nosort" >
        <a tal:condition="python: pCacheStatusReport.get( 'CanDiagnose', False)" 
            class="state-visible" id="cid_CacheDiagnosis_Link" href="#" tal:attributes="href python: '%s/MDDCacheDiagnostics/#cid_MDDCache_%s_section' % ( here.absolute_url(), pCacheName,)">
            <font size="2">
                <strong tal:content="python: here.MDDModelDDvlPlone_tool.fTranslateI18N( here, 'ModelDDvlPlone', 'ModelDDvlPlone_CacheDiagnostics_action_label', 'Diagnostics-',)" />
            </font>
        </a>
    </th>
    <th align="right" class="nosort" >
        <a tal:condition="python: pCacheStatusReport.get( 'CanInspect', False)" 
            class="state-visible" id="cid_InspectCache_Link" href="#" tal:attributes="href python: '%s/MDDInspectCache/?theCacheName=%s' % ( here.absolute_url(), pCacheName,)">
            <font size="2">
                <strong tal:content="python: here.MDDModelDDvlPlone_tool.fTranslateI18N( here, 'ModelDDvlPlone', 'ModelDDvlPlone_InspectCache_action_label', 'Inspect-',)" />
            </font>
        </a>
    </th>
</tr>
</thead>
<thead>
    <tr height="4" bgcolor="silver" ><td colspan="3" />
</thead>
<tbody tal:define="global unIndexClassFila    python: 0">
    <tr class="#" tal:attributes="class python: unasClasesFilas[unIndexClassFila % 2]"
        tal:define="global unIndexClassFila    python: unIndexClassFila + 1">
        <td align="left" colspan="2">
            <strong i18n:domain="ModelDDvlPlone" i18n:translate="ModelDDvlPlone_CacheStatus_LastFlushDate">Last Time the Cache was Flushed-</strong>
        </td>
        <td align="left"  >
            <tal:block tal:content="python: pCacheReport.get( 'LastFlushDate', '')" />
        </td>
    </tr>                                  
    <tr class="#" tal:attributes="class python: unasClasesFilas[unIndexClassFila % 2]"
        tal:define="global unIndexClassFila    python: unIndexClassFila + 1">
        <td  align="left" colspan="2">
            <strong i18n:domain="ModelDDvlPlone" i18n:translate="ModelDDvlPlone_CacheStatus_LastFlushingUser">Last User who Flushed the Cache-</strong>
        </td>
        <td  align="left"  >
            <tal:block tal:content="python: pCacheReport.get( 'LastFlushingUser', 0)" />
        </td>
    </tr> 
</tbody>
<thead>
    <tr height="4" bgcolor="silver" ><td colspan="3" /></tr>
    <tr>
        <th align="left"  class="nosort" valign="top" >
            <font size="2">
                <strong>
                    <span i18n:domain="ModelDDvlPlone" i18n:translate="ModelDDvlPlone_Cache_ActivityMetrics">Activity Metrics-</span>									
                </strong>
            </font>
        </th>
        <th align="left"  class="nosort" valign="top" >
            <font size="1">
                <strong>
                    <span i18n:domain="ModelDDvlPlone" i18n:translate="ModelDDvlPlone_Cache_Status_At">Status at-</span>									
                </strong>
                <br/>
                <span tal:content="python: pCacheReport.get( 'ReportTime_str', '')" />									
            </font>
        </th>
        <th align="left"  class="nosort" valign="top" >
            <font size="1" tal:condition="python: pCacheReport.get( 'PreviousReportTime_str', '')">
                <strong>
                    <span i18n:domain="ModelDDvlPlone" i18n:translate="ModelDDvlPlone_Cache_Delta_From">Delta from-</span>									
                </strong>
                <br/>
                <span tal:content="python: pCacheReport.get( 'PreviousReportTime_str', '')" />	
                <font color="red" tal:condition="python: pCacheReport.get( 'ReportTime_Delta', 0)">
                    <br/>
                    <span tal:content="python: pCacheReport.get( 'ReportTime_Delta_str', '')" />
                    sec
                    &emsp;&emsp;
                    <span tal:condition="python: not( ( pCacheReport.get( 'ReportTime_Delta_str', '') + ' sec') == pCacheReport.get( 'ReportTime_Delta_wstr', ''))"
                        tal:content="python: pCacheReport.get( 'ReportTime_Delta_wstr', '')" />
                </font>

    </font>
</th>
</tr>
</thead>
